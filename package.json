  "main": "./src/index.tsx",
  "scripts": {
    "build": "webpack build --define-process-env-node-env production",
    "build:analyze": "webpack build --define-process-env-node-env production && webpack --analyze",
    "dev": "webpack serve --define-process-env-node-env development --open",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit",
    "lint:all": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit && stylelint **/*.{css,scss} && prettier . --check",
    "lint:all:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix && tsc --noEmit && stylelint **/*.{css,scss} --fix && prettier . --write",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "ncu": "npm-check-updates --interactive --format group --doctor",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "stylelint": "stylelint **/*.{css,scss}",
    "stylelint:fix": "stylelint **/*.{css,scss} --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "minireset.css": "^0.0.7",
